(jbuild_version 1)

(library
 ((name brisk_cocoa)
  (public_name brisk.cocoa)
  (synopsis "Cocoa renderer and bindings")
  (libraries (brisk_core flex))
  (c_names (Cocoa_stubs))
  (c_flags (:standard
    (-Wextra -Wall -Werror -O -g -std=c99 -pedantic-errors -Wsign-compare -Wshadow)
    (-x objective-c)
  ))
 )
)
